# Production Environment Variables
# Copy this to .env.production.local and add your actual values

# API Configuration
VITE_511_API_KEY=
VITE_API_BASE_URL=https://api.511.org

# Application Settings
VITE_POLL_INTERVAL=60000
VITE_CACHE_TTL=30000
VITE_RATE_LIMIT_MAX_REQUESTS=60

# Feature Flags
VITE_ENABLE_WZDX=true
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_PWA=true

# Map Configuration
VITE_MAP_TILE_URL=https://{s}.tile.openstreetmap.org/{z}/{x}/{y}.png
VITE_MAP_DEFAULT_LAT=37.5
VITE_MAP_DEFAULT_LNG=-122.1
VITE_MAP_DEFAULT_ZOOM=10

# Monitoring (Optional)
VITE_SENTRY_DSN=
VITE_GA_TRACKING_ID=

# Build Configuration
VITE_BUILD_VERSION=${npm_package_version}
VITE_BUILD_TIME=${BUILD_TIME}
VITE_COMMIT_SHA=${COMMIT_SHA}
