{
  "name": "511-traffic-monitor",
  "version": "3.0.0",
  "description": "Production-ready real-time traffic monitoring for San Francisco Bay Area using 511.org API",
  "type": "module",
  "author": "Your Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/511-traffic-monitor.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/511-traffic-monitor/issues"
  },
  "keywords": [
    "traffic",
    "511",
    "bay-area",
    "real-time",
    "monitoring",
    "react",
    "typescript",
    "leaflet"
  ],
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:coverage-check": "vitest run --coverage && node scripts/check-coverage.js",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,css,md}\"",
    "prepare": "node -e \"try { require('husky').install() } catch (e) {if (e.code !== 'MODULE_NOT_FOUND') throw e}\"",
    "pre-commit": "npm run lint && npm run type-check && npm run test:run"
  },
  "dependencies": {
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "zustand": "4.4.7",
    "valtio": "1.13.0",
    "@tanstack/react-query": "5.17.0",
    "axios": "1.6.5",
    "zod": "3.22.4",
    "date-fns": "3.0.6",
    "clsx": "2.1.0",
    "dexie": "3.2.4",
    "dexie-react-hooks": "1.1.6",
    "comlink": "4.4.1",
    "socket.io-client": "4.7.2",
    "react-window": "1.8.10",
    "react-window-infinite-loader": "1.0.9",
    "@tanstack/react-virtual": "3.0.1",
    "leaflet": "1.9.4",
    "react-leaflet": "4.2.1",
    "leaflet.markercluster": "1.5.3",
    "axios": "1.6.5",
    "zod": "3.22.4",
    "date-fns": "3.0.6",
    "clsx": "2.1.0",
    "workbox-window": "7.0.0",
    "workbox-precaching": "7.0.0",
    "web-vitals": "3.5.1"
  },
  "devDependencies": {
    "typescript": "5.3.3",
    "vite": "5.0.11",
    "vite-plugin-pwa": "0.17.4",
    "@vitejs/plugin-react": "4.2.1",
    "@vitejs/plugin-legacy": "5.2.0",
    "@types/node": "20.10.6",
    "@types/react": "18.2.46",
    "@types/react-dom": "18.2.18",
    "@types/react-window": "1.8.8",
    "@types/leaflet": "1.9.8",
    "@types/leaflet.markercluster": "1.5.4",
    "vitest": "1.1.3",
    "@vitest/ui": "1.1.3",
    "@vitest/coverage-v8": "1.1.3",
    "jsdom": "23.2.0",
    "@testing-library/react": "14.1.2",
    "@testing-library/jest-dom": "6.1.5",
    "@testing-library/user-event": "14.5.1",
    "msw": "2.0.11",
    "eslint": "8.56.0",
    "@typescript-eslint/eslint-plugin": "6.18.1",
    "@typescript-eslint/parser": "6.18.1",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-react-refresh": "0.4.5",
    "eslint-config-prettier": "9.1.0",
    "prettier": "3.1.1",
    "autoprefixer": "10.4.16",
    "postcss": "8.4.32",
    "tailwindcss": "3.4.0",
    "husky": "8.0.3",
    "lint-staged": "15.2.0"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,md}": [
      "prettier --write"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
